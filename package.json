{
  "name": "myapp",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel serve ./src/index.html",
    "build": "PARCEL_BUNDLE_ANALYZER=1 API_KEY=hello parcel build ./src/index.html",
    "clean": "rm -rf ./dist/ ./parcel-bundle-reports/",
    "typecheck": "tsc --noEmit",
    "fmt": "prettier --list-different --write './src/**/*.{js,ts,jsx,tsx,css,scss,html}'",
    "eslint": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "stylelint": "stylelint './src/**/*.{css,scss}'"
  },
  "devDependencies": {
    "@oursky/eslint-plugin": "3.2.0",
    "@oursky/stylelint-oursky": "1.0.1",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "eslint": "7.7.0",
    "parcel": "2.0.0-beta.1",
    "postcss-modules": "3.2.0",
    "prettier": "2.0.5",
    "sass": "1.26.10",
    "stylelint": "13.6.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "react": "17.0.0-rc.0",
    "react-dom": "17.0.0-rc.0"
  }
}
